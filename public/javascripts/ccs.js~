      ]
	     }   
	 })

    function toggleCss(id,att){
       console.log('123')
    if ($(id).css(att) === 'auto')
    	{
        $(id).css({att:'1'});
        console.log(att);
     }
    else{
    $(id).css({att: 'auto'});
    console.log('not auto');	 
    } 
    $('.pservice').addClass('activetab');
     }

$(document).ready(function(){
	console.log('12345');
	$('#tser1').on("click",toggleCss('#tbod1','z-index'));
	$('#tser2').on("click",toggleCss('#tbod2','z-index'));
    });




