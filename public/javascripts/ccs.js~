   addthis.layers({
	     'theme' : 'transparent', 
	     'follow' : {
		 'services' : [
    {'service': 'facebook', 'id': 'Caribbean CAD Sourcing'},
    {'service': 'twitter', 'id': 'Caribbean CAD Sourcing'},
    {'service': 'google_follow', 'id': 'Caribbean CAD Sourcing'},
